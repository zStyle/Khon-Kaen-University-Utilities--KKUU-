<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent">
	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
		<ImageView android:layout_width="wrap_content"
			android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
			android:src="@drawable/emblemkkulogo"></ImageView>
		<TextView android:textSize="16dp" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="@string/kkutitle" android:layout_gravity="center_horizontal"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="@string/kkuinfo"></TextView>
		<Button android:id="@+id/moreinfo" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="@string/more" android:layout_gravity="center_horizontal"></Button>
	</LinearLayout>
</LinearLayout>
